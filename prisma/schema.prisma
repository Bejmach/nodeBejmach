generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Wpis{
	id	Int	@id @default(autoincrement())
	user_name	String
	post	String
}

model Kategoria{
	id	Int	@id @default(autoincrement())
	name	String	@unique
}

model Komentarz{
	id	Int	@id @default(autoincrement())
	user_name	String
	comment	String
}
